{
  "name": "react-tabllist",
  "version": "1.7.1",
  "description": "React-based list (table) components that support events, callback functions, and custom styles.",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --env development --config build/webpack.config.dev.js",
    "test": "jest --coverage --detectOpenHandles --forceExit && codecov",
    "tjest": "jest --detectOpenHandles --forceExit",
    "clean-demo": "rm -rf examples/",
    "clean-dist": "rm -rf dist/",
    "start-demo": "webpack serve --env development --config build/webpack.config.dev.demo.js",
    "demo": "yarn clean-demo && webpack --config build/webpack.config.prod.demo.js",
    "push-demo": "git subtree push -P examples github whatever:gh-pages && git subtree push -P examples coding whatever:coding-pages",
    "push": "git push github master && git push coding master",
    "build": "yarn clean-dist && webpack --config build/webpack.config.prod.js",
    "lint": "stylelint-config-prettier-check"
  },
  "keywords": [
    "react",
    "table",
    "list",
    "react-table",
    "react-tabllist",
    "tabllist",
    "react-component",
    "data-driven",
    "configurable",
    "custom-configuration",
    "visual-component",
    "multi-mode",
    "infinite-scroll",
    "visualization",
    "event-driven",
    "props",
    "subarray",
    "callback",
    "callback-functions",
    "jsx"
  ],
